<%reviews.forEach((review) => { %>
  <div class="box">

    <div class="row sp-btwn center-vertically">
      <a href="/albums/<%= review.album_id %>"><h2><%= review.title %></h2></a>
      <h3><%= review.date_created %></h3>
      <img src="/pictures/trash.png" alt="delete" class="delete-review" data-review-id='#'>
    </div>

    <p><%= review.content %></p>

    <p>by <a href="/users/<%= review.user_id %>"><%= review.name %></a></p>
  </div>
<% }) %>
